    "create-hash": "^1.2.0",
    "next": "^7.0.2",
    "path-match": "^1.2.4",
    "prop-types": "^15.6.1",
    "react": "^16.5.2",
    "react-app-polyfill": "^0.1.3",
    "react-dom": "^16.5.2",
    "react-jazzicon": "^0.1.2",
    "react-no-ssr": "^1.1.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
