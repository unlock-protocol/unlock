{
  "name": "unlock-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "bn.js": "^4.11.8",
    "bootstrap": "^4.0.0",
    "color-scheme": "^1.0.1",
    "prop-types": "^15.6.1",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-jazzicon": "^0.1.2",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-scripts": "1.1.4",
    "react-test-renderer": "^16.3.2",
    "redux": "^3.7.2",
    "styled-components": "^3.4.6",
    "truffle": "^4.1.11",
    "web3": "^1.0.0-beta.34",
    "web3-utils": "^1.0.0-beta.34"
  },
  "devDependencies": {
    "@storybook/addon-jest": "^3.4.10",
    "@storybook/react": "^3.4.10",
    "@svgr/cli": "^2.3.0",
    "babel-core": "^6.26.3",
    "enzyme": "^3.4.0",
    "enzyme-adapter-react-16": "^1.2.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-mocha": "^4.12.1",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "expect-enzyme": "^1.3.0",
    "jest-enzyme": "^6.0.3",
    "nock": "^9.3.3",
    "react-router-test-context": "^0.1.0",
    "redux-mock-store": "^1.5.1"
  },
  "engines": {
    "node": "=8.11.4"
  },
  "engineStrict": true,
  "scripts": {
    "start-app": "react-scripts start",
    "start-ganache": "ganache-cli --deterministic &",
    "deploy-lock": "cd ../smart-contracts && truffle deploy",
    "prestart": "npm run start-ganache && npm run deploy-lock && npm run serve-unlockjs",
    "start": "react-scripts start",
    "deploy-staging": "npm run build && TARGET=staging npm run build-unlockjs && aws s3 sync build/ s3://staging.unlock-protocol.com --profile unlock --no-progress | awk '{print $2;}' | cut -d'/' -f2- | awk '{print \"/\"$0}' | xargs aws cloudfront create-invalidation --distribution-id E1ZWU0E90B9EBN --paths --profile unlock",
    "build": "react-scripts build",
    "build-unlockjs": "./node_modules/webpack/bin/webpack.js --config webpack-unlockjs.config.js",
    "serve-unlockjs": "TARGET=dev node_modules/.bin/webpack-dev-server --config webpack-unlockjs.config.js &",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "lint": "./node_modules/eslint/bin/eslint.js .",
    "storybook": "start-storybook -p 9001 -c .storybook -s public",
    "svg-2-components": "./node_modules/@svgr/cli/bin/svgr --no-dimensions -d src/components/interface/svg/ public/images/icons/"
  }
}
