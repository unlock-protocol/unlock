    "web3-utils": "^1.0.0-beta.34"
  },
  "devDependencies": {
    "@storybook/addon-jest": "^4.0.0-alpha.24",
    "@storybook/react": "4.0.0-alpha.24",
    "@svgr/cli": "^3.1.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-styled-components": "1.5.1",
    "enzyme": "3.4.3",
    "enzyme-adapter-react-16": "1.2.0",
    "eslint": "^5.7.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-mocha": "^5.2.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-standard": "^4.0.0",
    "expect-enzyme": "^1.3.0",
    "jest": "^23.6.0",
    "jest-enzyme": "^7.0.0",
    "nock": "^10.0.1",
    "nodemon": "^1.12.1",
    "react-addons-test-utils": "15.6.2",
    "react-router-test-context": "^0.1.0",
    "react-test-renderer": "16.4.2",
    "redux-devtools-extension": "^2.13.5",
    "redux-mock-store": "^1.5.1",
    "storybook-react-router": "^1.0.1",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9"
  },
  "engines": {
    "node": "=8.11.4"
  },
  "engineStrict": true,
  "scripts": {
    "dev": "npm run prestart && nodemon src/server.js",
    "build": "next build src",
    "start": "NODE_ENV=production node src/server.js",
    "start-ganache": "ganache-cli -b 3 -i 1984 --deterministic &",
    "deploy-lock": "cd ../smart-contracts && truffle deploy --reset",
    "prestart": "npm run start-ganache && npm run deploy-lock && npm run serve-unlockjs",
    "deploy-staging": "npm run build && TARGET=staging npm run build-unlockjs && aws s3 sync build/ s3://staging.unlock-protocol.com --profile unlock --no-progress | awk '{print $2;}' | cut -d'/' -f2- | awk '{print \"/\"$0}' | xargs aws cloudfront create-invalidation --distribution-id E1ZWU0E90B9EBN --paths --profile unlock",
    "build-unlockjs": "./node_modules/webpack/bin/webpack.js --config webpack-unlockjs.config.js",
    "serve-unlockjs": "TARGET=dev node_modules/.bin/webpack-dev-server --config webpack-unlockjs.config.js &",
    "test": "NODE_ENV=test jest --env=jsdom",
    "lint": "./node_modules/eslint/bin/eslint.js .",
    "storybook": "start-storybook -p 9001 -c .storybook -s src/static",
    "svg-2-components": "./node_modules/@svgr/cli/bin/svgr --no-dimensions -d src/components/interface/svg/ src/static/images/svg/"
  },
  "nodemonConfig": {
    "watch": [
      "src/server.js"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
