{
  "name": "unlock-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@babel/core": "7.4.3",
    "@storybook/addon-actions": "5.0.8",
    "@storybook/addon-knobs": "5.0.8",
    "@storybook/addon-storyshots": "5.0.8",
    "@storybook/addon-viewport": "5.0.8",
    "@storybook/addons": "5.0.8",
    "@storybook/react": "5.0.8",
    "@types/jest": "^24.0.11",
    "@types/next": "^8.0.3",
    "@types/react": "16.8.13",
    "@types/react-redux": "7.0.7",
    "@types/storybook__react": "^4.0.1",
    "@types/styled-components": "4.1.14",
    "@types/web3": "^1.0.18",
    "@unlock-protocol/unlock-js": "0.0.14",
    "@zeit/next-source-maps": "0.0.3",
    "@zeit/next-typescript": "^1.1.1",
    "axios": "^0.18.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-require-context-hook": "^1.0.0",
    "babel-plugin-styled-components": "1.10.0",
    "color-scheme": "^1.0.1",
    "connected-next-router": "^1.0.1",
    "cross-env": "^5.2.0",
    "email-validator": "^2.0.4",
    "fork-ts-checker-webpack-plugin": "^1.0.0",
    "isomorphic-unfetch": "^3.0.0",
    "jest-dom": "3.1.3",
    "jest-mock-axios": "2.4.0",
    "jest-styled-components": "^6.3.1",
    "jsonwebtoken": "^8.4.0",
    "next": "8.0.4",
    "nock": "^10.0.5",
    "path-match": "^1.2.4",
    "prop-types": "15.7.2",
    "react": "^16.8.6",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dom": "^16.8.6",
    "react-jazzicon": "^0.1.2",
    "react-redux": "^7.0.2",
    "react-test-renderer": "^16.8.6",
    "react-testing-library": "6.1.2",
    "redux": "^4.0.1",
    "rss": "^1.2.2",
    "run-script-os": "^1.0.5",
    "storybook-chromatic": "^1.3.2",
    "styled-components": "4.2.0",
    "ts-jest": "24.0.2",
    "typescript": "3.4.3",
    "unlock-abi-0-1": "^1.0.0",
    "unlock-abi-0-2": "^1.0.0",
    "web3": "1.0.0-beta.37",
    "web3-utils": "1.0.0-beta.37"
  },
  "devDependencies": {
    "@svgr/cli": "4.2.0",
    "depcheck": "^0.7.1",
    "nodemon": "1.18.11",
    "npm-check": "^5.9.0"
  },
  "engines": {
    "node": "=8.11.4"
  },
  "engineStrict": true,
  "scripts": {
    "predev": "npm run start-ganache && npm run deploy-unlock-contract",
    "dev": "nodemon src/server.js",
    "build": "next build src",
    "deploy": "next export src -o out",
    "deploy-netlify": "./scripts/deploy-netlify.sh",
    "deploy-unlock-contract": "node scripts/deploy-unlock.js",
    "start": "cross-env NODE_ENV=production node src/server.js",
    "start-ganache": "run-script-os",
    "start-ganache:darwin:freebsd:linux:sunos": "cd .. && (npm run start-ganache -- -b 3 &) ",
    "start-ganache:win32": "cd .. && (START /b npm run start-ganache -- -b 3 ) ",
    "test": "cross-env UNLOCK_ENV=test jest --env=jsdom",
    "lint": "eslint --ext .tsx,.ts,.js src/",
    "storybook": "start-storybook -p 9001 -c .storybook -s src",
    "chromatic": "chromatic test --exit-zero-on-changes",
    "svg-2-components": "./node_modules/@svgr/cli/bin/svgr --title-prop --no-dimensions --template src/components/interface/svg/template.js --no-dimensions -d src/components/interface/svg/ src/static/images/svg/",
    "ci": "npm run lint && npm test",
    "depcheck": "depcheck --parsers=\"*.js:react,*.js:es6\"",
    "npm-check": "npm-check -u",
    "tsc": "tsc"
  },
  "lint-staged": {
    "linters": {
      "*.{js,ts,tsx}": [
        "eslint --fix",
        "git add"
      ]
    }
  },
  "nodemonConfig": {
    "watch": [
      "src/server.js"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
