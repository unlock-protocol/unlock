{
  "name": "unlock-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@babel/core": "7.4.5",
    "@storybook/addon-actions": "5.1.7",
    "@storybook/addon-knobs": "5.1.7",
    "@storybook/addon-storyshots": "5.1.7",
    "@storybook/addon-viewport": "5.1.7",
    "@storybook/addons": "5.1.7",
    "@storybook/react": "5.1.7",
    "@types/jest": "24.0.14",
    "@types/next": "8.0.5",
    "@types/react": "16.8.22",
    "@types/storybook__react": "4.0.2",
    "@types/styled-components": "4.1.16",
    "@zeit/next-source-maps": "0.0.3",
    "@zeit/next-typescript": "1.1.1",
    "babel-loader": "8.0.6",
    "babel-plugin-require-context-hook": "1.0.0",
    "babel-plugin-styled-components": "1.10.0",
    "cross-env": "5.2.0",
    "dotenv": "8.0.0",
    "fork-ts-checker-webpack-plugin": "1.3.7",
    "isomorphic-unfetch": "3.0.0",
    "jest-dom": "3.5.0",
    "jest-styled-components": "6.3.1",
    "next": "8.1.0",
    "path-match": "1.2.4",
    "prop-types": "15.7.2",
    "raw-loader": "3.0.0",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-ga": "2.5.7",
    "react-intercom": "1.0.15",
    "react-redux": "7.1.0",
    "react-showdown": "1.6.0",
    "react-test-renderer": "16.8.6",
    "react-testing-library": "6.1.2",
    "rss": "1.2.2",
    "run-script-os": "1.0.5",
    "storybook-chromatic": "1.3.3",
    "styled-components": "4.3.1",
    "terser": "4.0.0",
    "ts-jest": "24.0.2",
    "typescript": "3.5.2",
    "yaml-front-matter": "4.0.0"
  },
  "devDependencies": {
    "@svgr/cli": "4.3.0",
    "nodemon": "1.19.1"
  },
  "engines": {
    "node": "8.16.0"
  },
  "engineStrict": true,
  "scripts": {
    "predev": "npm run build-blog",
    "dev": "nodemon src/server.js",
    "build": "next build src",
    "build-blog": "node scripts/build-blog.js",
    "deploy": "next export src -o out",
    "deploy-netlify": "./scripts/deploy-netlify.sh",
    "start": "cross-env NODE_ENV=production node src/server.js",
    "test": "cross-env UNLOCK_ENV=test jest --env=jsdom",
    "lint": "eslint --ext .tsx,.ts,.js src/",
    "storybook": "start-storybook -p 9001 -c .storybook -s src",
    "svg-2-components": "./node_modules/@svgr/cli/bin/svgr --title-prop --no-dimensions --template src/components/interface/svg/template.js --no-dimensions -d src/components/interface/svg/ src/static/images/svg/",
    "ci": "npm run lint && npm test",
    "tsc": "tsc"
  },
  "lint-staged": {
    "linters": {
      "*.{js,ts,tsx}": [
        "eslint --fix",
        "git add"
      ]
    },
    "ignore": [
      "*.js.snap"
    ]
  },
  "nodemonConfig": {
    "watch": [
      "src/server.js"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
