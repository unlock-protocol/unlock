{
  "name": "@unlock-protocol/paywall",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "predev": "yarn before",
    "dev": "nodemon src/server.js",
    "before": "yarn build-script",
    "build": "yarn before && next build src",
    "predeploy": "yarn run build",
    "deploy": "next export src -o out",
    "deploy-netlify": "./scripts/deploy-netlify.sh",
    "prestart": "yarn run build",
    "start": "yarn build-script && NODE_ENV=production node src/server.js",
    "test": "UNLOCK_ENV=test jest --env=jsdom",
    "lint": "eslint --resolve-plugins-relative-to ../packages/eslint-config --ext .tsx,.ts,.js src/",
    "build-script": "yarn build-unlock.latest.js",
    "watch-script": "webpack --watch --config unlock.latest.js.webpack.config.js",
    "build-unlock.latest.js": "webpack --config unlock.latest.js.webpack.config.js",
    "ci": "yarn test && yarn lint"
  },
  "dependencies": {
    "@types/next": "8.0.7",
    "@types/node": "14.17.14",
    "@types/postmate": "1.5.2",
    "@types/react": "17.0.19",
    "@types/react-dom": "17.0.9",
    "@types/styled-components": "5.1.11",
    "@zeit/next-css": "1.0.1",
    "@zeit/next-source-maps": "0.0.3",
    "dotenv": "10.0.0",
    "lint-staged": "11.0.0",
    "next": "10.2.3",
    "path-match": "1.2.4",
    "postmate": "1.5.2",
    "prop-types": "15.7.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-test-renderer": "17.0.2",
    "style-loader": "3.0.0",
    "styled-components": "5.2.3"
  },
  "devDependencies": {
    "@babel/core": "7.14.8",
    "@svgr/cli": "5.5.0",
    "@testing-library/jest-dom": "5.12.0",
    "@testing-library/react": "11.2.7",
    "@types/jest": "26.0.24",
    "@unlock-protocol/eslint-config" : "0.0.1",
    "babel-jest": "27.0.6",
    "babel-loader": "8.2.2",
    "babel-plugin-require-context-hook": "1.0.0",
    "babel-preset-react-app": "10.0.0",
    "css-loader": "6.2.0",
    "eslint": "7.25.0",
    "jest": "26.6.3",
    "jest-fetch-mock": "3.0.3",
    "jest-styled-components": "7.0.5",
    "nodemon": "2.0.12",
    "prettier": "2.2.1",
    "ts-jest": "26.5.6",
    "ts-loader": "9.2.5",
    "typescript": "4.2.4",
    "webpack": "5.36.2",
    "webpack-cli": "4.6.0"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "nodemonConfig": {
    "watch": [
      "src/server.js"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
