{
  "name": "unlock-paywall",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "predev": "yarn before",
    "dev": "nodemon src/server.js",
    "before": "yarn build-checkout",
    "build": "yarn before && next build src",
    "deploy": "next export src -o out",
    "deploy-netlify": "./scripts/deploy-netlify.sh",
    "start": "yarn build-checkout && cross-env NODE_ENV=production node src/server.js",
    "test": "cross-env UNLOCK_ENV=test jest --env=jsdom",
    "lint": "eslint --ext .tsx,.ts,.js src/",
    "svg-2-components": "./node_modules/@svgr/cli/bin/svgr --title-prop --no-dimensions --template src/components/interface/svg/template.js --no-dimensions -d src/components/interface/svg/ src/static/images/svg/",
    "build-checkout": "yarn build-unlock.latest.js",
    "watch-checkout": "webpack --watch --config unlock.latest.js.webpack.config.js",
    "build-unlock.latest.js": "webpack --config unlock.latest.js.webpack.config.js",
    "storybook": "start-storybook -p 9002 -c .storybook -s .",
    "ci": "yarn test && yarn lint"
  },
  "dependencies": {
    "@babel/core": "7.9.6",
    "@storybook/addon-actions": "5.3.18",
    "@storybook/addon-knobs": "5.3.18",
    "@storybook/addon-storyshots": "5.3.18",
    "@storybook/addon-viewport": "5.3.18",
    "@storybook/addons": "5.3.18",
    "@storybook/react": "5.3.18",
    "@testing-library/jest-dom": "5.5.0",
    "@testing-library/react": "10.0.4",
    "@testing-library/react-hooks": "3.2.1",
    "@types/jest": "25.2.1",
    "@types/next": "8.0.7",
    "@types/node": "12.12.37",
    "@types/postmate": "1.5.0",
    "@types/react": "16.9.34",
    "@types/react-dom": "16.9.7",
    "@types/styled-components": "5.1.0",
    "@typescript-eslint/eslint-plugin": "2.30.0",
    "@typescript-eslint/parser": "2.30.0",
    "@unlock-protocol/unlock-js": "0.11.0",
    "@zeit/next-css": "1.0.1",
    "@zeit/next-source-maps": "0.0.3",
    "babel-eslint": "10.1.0",
    "babel-jest": "25.5.1",
    "babel-loader": "8.1.0",
    "babel-plugin-require-context-hook": "1.0.0",
    "babel-preset-react-app": "9.1.2",
    "cross-env": "7.0.2",
    "css-loader": "3.5.3",
    "dotenv": "8.2.0",
    "eslint": "6.8.0",
    "jest": "25.5.4",
    "jest-fetch-mock": "3.0.3",
    "jest-styled-components": "7.0.2",
    "lint-staged": "10.2.2",
    "netlify-cli": "2.48.0",
    "next": "9.3.6",
    "path-match": "1.2.4",
    "postmate": "1.5.2",
    "prettier": "1.19.1",
    "prop-types": "15.7.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-hook-form": "5.6.1",
    "react-test-renderer": "16.13.1",
    "style-loader": "1.2.1",
    "styled-components": "5.1.0",
    "ts-jest": "25.4.0",
    "ts-loader": "7.0.2",
    "typescript": "3.8.3",
    "webpack": "4.43.0",
    "validator": "13.0.0",
    "webpack-cli": "3.3.11"
  },
  "devDependencies": {
    "@svgr/cli": "5.4.0",
    "nodemon": "2.0.3"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "nodemonConfig": {
    "watch": [
      "src/server.js"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}