{
  "name": "unlock-paywall",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "predev": "yarn before",
    "dev": "nodemon src/server.js",
    "before": "yarn build-checkout",
    "build": "yarn before && next build src",
    "deploy": "next export src -o out",
    "deploy-netlify": "./scripts/deploy-netlify.sh",
    "start": "yarn build-checkout && cross-env NODE_ENV=production node src/server.js",
    "test": "cross-env UNLOCK_ENV=test jest --env=jsdom",
    "lint": "eslint --ext .tsx,.ts,.js src/",
    "svg-2-components": "./node_modules/@svgr/cli/bin/svgr --title-prop --no-dimensions --template src/components/interface/svg/template.js --no-dimensions -d src/components/interface/svg/ src/static/images/svg/",
    "build-checkout": "yarn build-unlock.1.0.js && yarn build-data-iframe.1.0.js",
    "build-unlock.1.0.js": "webpack --config unlock.1.0.js.webpack.config.js",
    "build-unlock-web": "cross-env UNLOCK_ENV=prod webpack --config unlock-web-module.webpack.config.js",
    "build-data-iframe.1.0.js": "webpack --config data-iframe.1.0.webpack.config.js",
    "build-data-iframe.2.0.js": "webpack --config data-iframe.2.0.webpack.config.js",
    "storybook": "start-storybook -p 9002 -c .storybook -s .",
    "ci": "yarn test && yarn lint"
  },
  "dependencies": {
    "@babel/core": "7.7.7",
    "@storybook/addon-actions": "5.2.8",
    "@storybook/addon-knobs": "5.2.8",
    "@storybook/addon-storyshots": "5.2.8",
    "@storybook/addon-viewport": "5.2.8",
    "@storybook/addons": "5.2.8",
    "@storybook/react": "5.2.8",
    "@testing-library/jest-dom": "4.2.4",
    "@testing-library/react": "9.4.0",
    "@types/jest": "24.0.25",
    "@types/next": "8.0.7",
    "@types/node": "12.12.24",
    "@types/postmate": "1.5.0",
    "@types/react": "16.9.17",
    "@types/react-dom": "16.9.4",
    "@types/styled-components": "4.4.1",
    "@typescript-eslint/eslint-plugin": "2.14.0",
    "@typescript-eslint/parser": "2.14.0",
    "@unlock-protocol/unlock-js": "0.5.4",
    "@zeit/next-css": "1.0.1",
    "@zeit/next-source-maps": "0.0.3",
    "axios": "0.19.0",
    "babel-eslint": "10.0.3",
    "babel-jest": "24.9.0",
    "babel-loader": "8.0.6",
    "babel-plugin-require-context-hook": "1.0.0",
    "cross-env": "6.0.3",
    "css-loader": "3.4.1",
    "cssnano": "4.1.10",
    "dotenv": "8.2.0",
    "eslint": "6.8.0",
    "fork-ts-checker-webpack-plugin": "3.1.1",
    "jest": "24.9.0",
    "jest-mock-axios": "3.2.0",
    "jest-styled-components": "6.3.4",
    "lint-staged": "8.2.1",
    "netlify-cli": "2.25.0",
    "next": "9.1.6",
    "path-match": "1.2.4",
    "postcss-nested": "4.2.1",
    "postcss-simple-vars": "5.0.2",
    "postmate": "1.5.2",
    "prettier": "1.19.1",
    "prop-types": "15.7.2",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-hooks-testing-library": "0.6.0",
    "react-test-renderer": "16.12.0",
    "rollup": "1.28.0",
    "rollup-plugin-babel": "4.3.3",
    "rollup-plugin-postcss": "2.0.3",
    "rollup-plugin-terser": "5.1.3",
    "strict-event-emitter-types": "2.0.0",
    "style-loader": "1.1.2",
    "styled-components": "4.4.1",
    "ts-jest": "24.2.0",
    "ts-loader": "6.2.1",
    "typescript": "3.7.4",
    "validator": "12.1.0",
    "webpack": "4.41.5",
    "webpack-cli": "3.3.10"
  },
  "devDependencies": {
    "@svgr/cli": "5.0.1",
    "nodemon": "2.0.2"
  },
  "lint-staged": {
    "linters": {
      "*.{js,ts,tsx}": [
        "eslint --fix",
        "git add"
      ]
    }
  },
  "nodemonConfig": {
    "watch": [
      "src/server.js"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
