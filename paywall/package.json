{
  "name": "unlock-paywall",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "predev": "yarn before",
    "dev": "nodemon src/server.js",
    "before": "yarn build-script",
    "build": "yarn before && next build src",
    "predeploy": "yarn run build",
    "deploy": "next export src -o out",
    "deploy-netlify": "./scripts/deploy-netlify.sh",
    "prestart": "yarn run build",
    "start": "yarn build-script && NODE_ENV=production node src/server.js",
    "test": "UNLOCK_ENV=test jest --env=jsdom",
    "lint": "eslint --ext .tsx,.ts,.js src/",
    "build-script": "yarn build-unlock.latest.js",
    "watch-script": "webpack --watch --config unlock.latest.js.webpack.config.js",
    "build-unlock.latest.js": "webpack --config unlock.latest.js.webpack.config.js",
    "storybook": "start-storybook -p 9002 -c .storybook -s .",
    "ci": "yarn test && yarn lint"
  },
  "dependencies": {
    "@babel/core": "7.13.14",
    "@storybook/addon-actions": "6.2.2",
    "@storybook/addon-knobs": "6.2.2",
    "@storybook/addon-storyshots": "6.2.2",
    "@storybook/addon-viewport": "6.2.2",
    "@storybook/react": "6.2.2",
    "@testing-library/jest-dom": "5.11.10",
    "@testing-library/react": "11.2.6",
    "@types/jest": "26.0.22",
    "@types/next": "8.0.7",
    "@types/node": "12.20.7",
    "@types/postmate": "1.5.1",
    "@types/react": "17.0.3",
    "@types/react-dom": "17.0.3",
    "@types/styled-components": "5.1.9",
    "@unlock-protocol/unlock-js": "0.15.2",
    "@zeit/next-css": "1.0.1",
    "@zeit/next-source-maps": "0.0.3",
    "babel-jest": "26.6.3",
    "babel-loader": "8.2.2",
    "babel-plugin-require-context-hook": "1.0.0",
    "babel-preset-react-app": "10.0.0",
    "css-loader": "5.2.0",
    "dotenv": "8.2.0",
    "jest": "26.6.3",
    "jest-fetch-mock": "3.0.3",
    "jest-styled-components": "7.0.3",
    "lint-staged": "10.5.4",
    "netlify-cli": "3.21.12",
    "next": "10.1.3",
    "path-match": "1.2.4",
    "postmate": "1.5.2",
    "prettier": "2.2.1",
    "prop-types": "15.7.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-test-renderer": "17.0.2",
    "style-loader": "2.0.0",
    "styled-components": "5.2.3",
    "ts-jest": "26.5.4",
    "ts-loader": "8.1.0",
    "typescript": "4.2.3",
    "webpack": "5.30.0",
    "webpack-cli": "4.6.0"
  },
  "devDependencies": {
    "@svgr/cli": "5.5.0",
    "nodemon": "2.0.7"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "nodemonConfig": {
    "watch": [
      "src/server.js"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
