{
  "name": "unlock-paywall",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "predev": "yarn before",
    "dev": "nodemon src/server.js",
    "before": "yarn build-script",
    "build": "yarn before && next build src",
    "predeploy": "yarn run build",
    "deploy": "next export src -o out",
    "deploy-netlify": "./scripts/deploy-netlify.sh",
    "prestart": "yarn run build",
    "start": "yarn build-script && cross-env NODE_ENV=production node src/server.js",
    "test": "cross-env UNLOCK_ENV=test jest --env=jsdom",
    "lint": "eslint --ext .tsx,.ts,.js src/",
    "build-script": "yarn build-unlock.latest.js",
    "watch-script": "webpack --watch --config unlock.latest.js.webpack.config.js",
    "build-unlock.latest.js": "webpack --config unlock.latest.js.webpack.config.js",
    "storybook": "start-storybook -p 9002 -c .storybook -s .",
    "ci": "yarn test && yarn lint"
  },
  "dependencies": {
    "@babel/core": "7.10.2",
    "@storybook/addon-actions": "5.3.19",
    "@storybook/addon-knobs": "5.3.19",
    "@storybook/addon-storyshots": "5.3.19",
    "@storybook/addon-viewport": "5.3.19",
    "@storybook/react": "5.3.19",
    "@testing-library/jest-dom": "5.9.0",
    "@testing-library/react": "10.0.4",
    "@types/jest": "25.2.3",
    "@types/next": "8.0.7",
    "@types/node": "12.12.42",
    "@types/postmate": "1.5.0",
    "@types/react": "16.9.35",
    "@types/react-dom": "16.9.8",
    "@types/styled-components": "5.1.0",
    "@typescript-eslint/eslint-plugin": "3.1.0",
    "@typescript-eslint/parser": "3.1.0",
    "@unlock-protocol/unlock-js": "0.12.2",
    "@zeit/next-css": "1.0.1",
    "@zeit/next-source-maps": "0.0.3",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.0.1",
    "babel-loader": "8.1.0",
    "babel-plugin-require-context-hook": "1.0.0",
    "babel-preset-react-app": "9.1.2",
    "cross-env": "7.0.2",
    "css-loader": "3.5.3",
    "dotenv": "8.2.0",
    "eslint": "6.8.0",
    "jest": "25.5.4",
    "jest-fetch-mock": "3.0.3",
    "jest-styled-components": "7.0.2",
    "lint-staged": "10.2.7",
    "netlify-cli": "2.52.0",
    "next": "9.4.4",
    "path-match": "1.2.4",
    "postmate": "1.5.2",
    "prettier": "1.19.1",
    "prop-types": "15.7.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-test-renderer": "16.13.1",
    "style-loader": "1.2.1",
    "styled-components": "5.1.1",
    "ts-jest": "26.1.0",
    "ts-loader": "7.0.5",
    "typescript": "3.9.3",
    "webpack": "4.43.0",
    "webpack-cli": "3.3.11"
  },
  "devDependencies": {
    "@svgr/cli": "5.4.0",
    "nodemon": "2.0.4"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "nodemonConfig": {
    "watch": [
      "src/server.js"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}