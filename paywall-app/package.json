  "license": "MIT",
  "devDependencies": {
    "@unlock-protocol/eslint-config": "workspace:./packages/eslint-config",
    "@unlock-protocol/tsconfig": "workspace:./packages/tsconfig",
    "eslint": "8.57.0",
    "prettier": "3.3.3",
    "typescript": "5.5.4",
    "vite": "5.4.2"
  },
  "browserslist": [
    "defaults",
    "not IE 11",
    "maintained node versions"
