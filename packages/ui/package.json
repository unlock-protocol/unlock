{
  "name": "@unlock-protocol/ui",
  "description": "Unlock's design system",
  "files": [
    "dist"
  ],
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    },
    "./dist/style.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    },
    "./dist/unlock-tailwind-preset": {
      "require": "./dist/unlock-tailwind-preset.js"
    }
  },
  "version": "0.4.8",
  "scripts": {
    "preview": "yarn build && vite --config ./vite.config.ts src",
    "build": "yarn build:lib && yarn build:cdn && tsup ./lib/index.tsx --dts-only",
    "build:lib": "vite build",
    "build:cdn": "IS_CDN=1 && vite build",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "STORYBOOK=true storybook build"
  },
  "dependencies": {
    "@headlessui/react": "2.2.0",
    "@radix-ui/react-tooltip": "1.2.4",
    "@tailwindcss/aspect-ratio": "0.4.2",
    "@tailwindcss/forms": "0.5.10",
    "@tailwindcss/typography": "0.5.16",
    "@tanstack/react-query": "5.74.11",
    "@tw-classed/react": "1.8.0",
    "ethers": "6.13.5",
    "prism-react-renderer": "2.4.1",
    "react-dropzone": "14.3.8",
    "react-hook-form": "7.56.1",
    "react-hot-toast": "2.5.2",
    "react-icons": "5.5.0",
    "react-use-clipboard": "1.0.9",
    "tailwind-merge": "3.2.0",
    "tailwindcss": "3.4.17"
  },
  "peerDependencies": {
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/unlock-protocol/unlock.git",
    "directory": "packages/ui"
  },
  "devDependencies": {
    "@mdx-js/preact": "3.1.0",
    "@mdx-js/react": "3.1.0",
    "@storybook/addon-actions": "8.6.3",
    "@storybook/addon-docs": "8.6.3",
    "@storybook/addon-essentials": "8.6.3",
    "@storybook/addon-links": "8.6.3",
    "@storybook/react": "8.6.3",
    "@storybook/react-vite": "8.6.3",
    "@types/react": "18.3.18",
    "@types/react-dom": "18.3.5",
    "@unlock-protocol/unlock-js": "workspace:./packages/unlock-js",
    "@vitejs/plugin-react": "4.4.1",
    "autoprefixer": "10.4.21",
    "postcss": "8.5.3",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "storybook": "8.6.3",
    "tailwindcss": "3.4.17",
    "tsup": "8.4.0",
    "tsx": "4.19.3",
    "typescript": "5.8.2",
    "url-loader": "4.1.1",
    "vite": "6.2.6",
    "vite-plugin-node-polyfills": "0.23.0",
    "vite-plugin-svgr": "4.3.0",
    "vite-tsconfig-paths": "4.3.2"
  }
}
