    "@tw-classed/react": "1.5.1",
    "@unlock-protocol/networks": "workspace:./packages/networks",
    "ethers": "5.7.2",
    "prism-react-renderer": "2.0.4",
    "react-dropzone": "14.2.3",
    "react-hook-form": "7.45.1",
    "react-icons": "4.10.1",
