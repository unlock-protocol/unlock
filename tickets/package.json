{
  "name": "unlock-tickets",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "nodemon src/server.js",
    "build": "next build src",
    "deploy": "next export src -o out",
    "deploy-netlify": "./scripts/deploy-netlify.sh",
    "start": "cross-env NODE_ENV=production node src/server.js",
    "test": "cross-env UNLOCK_ENV=test jest --env=jsdom",
    "lint": "eslint --ext .tsx,.ts,.js src/",
    "svg-2-components": "./node_modules/@svgr/cli/bin/svgr --title-prop --no-dimensions --template src/components/interface/svg/template.js --no-dimensions -d src/components/interface/svg/ src/static/images/svg/",
    "storybook": "start-storybook -p 9003 -c .storybook -s src",
    "ci": "yarn test && yarn lint",
    "tsc": "tsc"
  },
  "dependencies": {
    "@babel/core": "7.9.0",
    "@storybook/addon-actions": "5.3.17",
    "@storybook/addon-knobs": "5.3.17",
    "@storybook/addon-storyshots": "5.3.17",
    "@storybook/addon-viewport": "5.3.17",
    "@storybook/addons": "5.3.17",
    "@storybook/react": "5.3.17",
    "@testing-library/jest-dom": "5.1.1",
    "@testing-library/react": "10.0.1",
    "@types/jest": "25.1.4",
    "@types/next": "8.0.7",
    "@types/react": "16.9.25",
    "@types/react-redux": "7.1.7",
    "@types/storybook__react": "4.0.2",
    "@types/styled-components": "5.0.1",
    "@unlock-protocol/unlock-js": "0.9.2",
    "@zeit/next-css": "1.0.1",
    "@zeit/next-source-maps": "0.0.3",
    "@zeit/next-typescript": "1.1.1",
    "axios": "0.19.2",
    "babel-jest": "25.1.0",
    "babel-loader": "8.1.0",
    "babel-plugin-require-context-hook": "1.0.0",
    "babel-preset-react-app": "9.1.2",
    "cross-env": "7.0.2",
    "dotenv": "8.2.0",
    "eslint": "6.8.0",
    "jest": "25.1.0",
    "jest-mock-axios": "3.2.0",
    "jest-styled-components": "7.0.0",
    "lint-staged": "10.0.8",
    "netlify-cli": "2.40.0",
    "next": "8.1.0",
    "path-match": "1.2.4",
    "prettier": "1.19.1",
    "prop-types": "15.7.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-redux": "7.2.0",
    "react-router": "5.1.2",
    "react-select": "3.1.0",
    "react-test-renderer": "16.13.1",
    "redux": "4.0.5",
    "styled-components": "5.0.1",
    "ts-jest": "25.2.1",
    "typescript": "3.8.3"
  },
  "devDependencies": {
    "@svgr/cli": "5.3.0",
    "nodemon": "2.0.2"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "nodemonConfig": {
    "watch": [
      "src/server.js"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "engines": {
    "node": "^10"
  },
  "engineStrict": true
}
