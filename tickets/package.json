{
  "name": "unlock-tickets",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "nodemon src/server.js",
    "build": "next build src",
    "deploy": "next export src -o out",
    "deploy-netlify": "./scripts/deploy-netlify.sh",
    "start": "cross-env NODE_ENV=production node src/server.js",
    "test": "cross-env UNLOCK_ENV=test jest --env=jsdom",
    "lint": "eslint --ext .tsx,.ts,.js src/",
    "svg-2-components": "./node_modules/@svgr/cli/bin/svgr --title-prop --no-dimensions --template src/components/interface/svg/template.js --no-dimensions -d src/components/interface/svg/ src/static/images/svg/",
    "storybook": "start-storybook -p 9003 -c .storybook -s src",
    "ci": "yarn test && yarn lint",
    "tsc": "tsc"
  },
  "dependencies": {
    "@babel/core": "7.7.7",
    "@storybook/addon-actions": "5.2.8",
    "@storybook/addon-knobs": "5.2.8",
    "@storybook/addon-storyshots": "5.2.8",
    "@storybook/addon-viewport": "5.2.8",
    "@storybook/addons": "5.2.8",
    "@storybook/react": "5.2.8",
    "@testing-library/jest-dom": "4.2.4",
    "@testing-library/react": "9.4.0",
    "@types/jest": "24.0.25",
    "@types/next": "8.0.7",
    "@types/react": "16.9.17",
    "@types/react-redux": "7.1.5",
    "@types/storybook__react": "4.0.2",
    "@types/styled-components": "4.4.1",
    "@unlock-protocol/unlock-js": "0.5.4",
    "@zeit/next-css": "1.0.1",
    "@zeit/next-source-maps": "0.0.3",
    "@zeit/next-typescript": "1.1.1",
    "axios": "0.19.0",
    "babel-jest": "24.9.0",
    "babel-loader": "8.0.6",
    "babel-plugin-require-context-hook": "1.0.0",
    "cross-env": "6.0.3",
    "dotenv": "8.2.0",
    "eslint": "6.8.0",
    "fork-ts-checker-webpack-plugin": "3.1.1",
    "jest": "24.9.0",
    "jest-mock-axios": "3.2.0",
    "jest-styled-components": "6.3.4",
    "lint-staged": "8.2.1",
    "netlify-cli": "2.25.0",
    "next": "8.1.0",
    "path-match": "1.2.4",
    "prettier": "1.19.1",
    "prop-types": "15.7.2",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-redux": "7.1.3",
    "react-router": "5.1.2",
    "react-select": "3.0.8",
    "react-test-renderer": "16.12.0",
    "redux": "4.0.5",
    "styled-components": "4.4.1",
    "ts-jest": "24.2.0",
    "typescript": "3.7.4"
  },
  "devDependencies": {
    "@svgr/cli": "5.0.1",
    "nodemon": "2.0.2"
  },
  "lint-staged": {
    "linters": {
      "*.{js,ts,tsx}": [
        "eslint --fix",
        "git add"
      ]
    }
  },
  "nodemonConfig": {
    "watch": [
      "src/server.js"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "engines": {
    "node": "^10"
  },
  "engineStrict": true
}
