{
  "name": "unlock-tickets",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "nodemon src/server.js",
    "predev": "npm run start-clean-locksmith",
    "start-clean-locksmith": "cd ../locksmith && (rm development.sqlite3; npm run dev &)",
    "build": "next build src",
    "deploy": "next export src -o out",
    "deploy-netlify": "./scripts/deploy-netlify.sh",
    "start": "cross-env NODE_ENV=production node src/server.js",
    "test": "cross-env UNLOCK_ENV=test jest --env=jsdom",
    "lint": "eslint --ext .tsx,.ts,.js src/",
    "svg-2-components": "./node_modules/@svgr/cli/bin/svgr --title-prop --no-dimensions --template src/components/interface/svg/template.js --no-dimensions -d src/components/interface/svg/ src/static/images/svg/",
    "storybook": "start-storybook -p 9003 -c .storybook -s .",
    "ci": "npm run lint && npm test",
    "tsc": "tsc"
  },
  "dependencies": {
    "@babel/core": "7.4.5",
    "@storybook/addon-actions": "5.1.9",
    "@storybook/addon-knobs": "5.1.9",
    "@storybook/addon-storyshots": "5.1.9",
    "@storybook/addon-viewport": "5.1.9",
    "@storybook/addons": "5.1.9",
    "@storybook/react": "5.1.9",
    "@types/jest": "24.0.15",
    "@types/next": "8.0.5",
    "@types/react": "16.8.22",
    "@types/react-redux": "7.1.1",
    "@types/storybook__react": "4.0.2",
    "@types/styled-components": "4.1.16",
    "@unlock-protocol/unlock-js": "0.3.10",
    "@zeit/next-css": "1.0.1",
    "@zeit/next-source-maps": "0.0.3",
    "@zeit/next-typescript": "1.1.1",
    "axios": "0.19.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-require-context-hook": "1.0.0",
    "connected-react-router": "6.4.0",
    "cross-env": "5.2.0",
    "dotenv": "8.0.0",
    "fork-ts-checker-webpack-plugin": "1.3.7",
    "history": "4.9.0",
    "jest-dom": "3.5.0",
    "jest-mock-axios": "3.0.0",
    "jest-styled-components": "6.3.3",
    "next": "8.1.0",
    "path-match": "1.2.4",
    "prop-types": "15.7.2",
    "qrcode.react": "0.9.3",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-redux": "7.1.0",
    "react-router": "5.0.1",
    "react-select": "3.0.4",
    "react-test-renderer": "16.8.6",
    "react-testing-library": "6.1.2",
    "redux": "4.0.1",
    "run-script-os": "1.0.7",
    "styled-components": "4.3.2",
    "ts-jest": "24.0.2",
    "typescript": "3.5.2"
  },
  "devDependencies": {
    "@svgr/cli": "4.3.1",
    "nodemon": "1.19.1"
  },
  "lint-staged": {
    "linters": {
      "*.{js,ts,tsx}": [
        "eslint --fix",
        "git add"
      ]
    }
  },
  "nodemonConfig": {
    "watch": [
      "src/server.js"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "engines": {
    "node": "8.16.0"
  },
  "engineStrict": true
}
