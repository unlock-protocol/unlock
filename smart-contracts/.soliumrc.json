{
  "extends": "solium:recommended",
  "plugins": [
    "security"
  ],
  "rules": {
    "quotes": [
      "warning",
      "single"
    ],
    "indentation": [
      "warning",
      2
    ],
    "linebreak-style": [
      "error",
      "unix"
    ],
    "security/no-assign-params": "warning"
  }
}