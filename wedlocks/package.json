{
  "name": "@unlock-protocol/wedlocks",
  "version": "1.0.0",
  "description": "The email server for Unlock",
  "main": "src/server.js",
  "dependencies": {
    "@babel/cli": "7.19.3",
    "@babel/core": "7.20.5",
    "@babel/node": "7.20.5",
    "@babel/preset-env": "7.20.2",
    "@typescript-eslint/parser": "5.47.0",
    "@unlock-protocol/eslint-config": "workspace:./packages/eslint-config",
    "babel-eslint": "10.1.0",
    "babel-jest": "28.1.3",
    "babel-loader": "9.1.0",
    "babel-plugin-wildcard": "7.0.0",
    "dotenv": "16.0.3",
    "eslint": "8.30.0",
    "handlebars": "4.7.7",
    "jest": "27.5.1",
    "lint-staged": "13.1.0",
    "netlify-cli": "12.4.0",
    "node-forge": "1.3.1",
    "nodemailer": "6.8.0",
    "prettier": "2.8.1",
    "ts-jest": "27.1.5",
    "typescript": "4.9.4",
    "winston": "3.8.2"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "./setupTests.js"
    ],
    "testPathIgnorePatterns": [
      "__tests__/utils.js"
    ]
  },
  "lint-staged": {
    ".{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "scripts": {
    "build": "netlify build",
    "dev": "UNLOCK_ENV=dev netlify functions:serve",
    "test": "UNLOCK_ENV=test jest",
    "testvi": "UNLOCK_ENV=test vitest run --no-threads --coverage --environment=node",
    "lint": "eslint --resolve-plugins-relative-to ../packages/eslint-config --ext .ts,.js src/",
    "ci": "yarn test && yarn lint",
    "deploy-netlify": "./scripts/deploy-netlify.sh"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/plugin-transform-runtime": "7.19.6",
    "@testing-library/jest-dom": "5.16.5",
    "regenerator-runtime": "0.13.11",
    "vitest": "0.26.2",
    "@vitest/coverage-c8": "0.26.2"
  }
}
