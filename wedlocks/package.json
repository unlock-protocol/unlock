{
  "name": "wedlocks",
  "version": "1.0.0",
  "description": "The email server for Unlock",
  "main": "src/server.js",
  "dependencies": {
    "@babel/cli": "7.14.8",
    "@babel/node": "7.14.9",
    "@babel/preset-env": "7.14.9",
    "@unlock-protocol/eslint-config": "workspace:*",
    "@unlock-protocol/tooling": "workspace:*",
    "aws-sdk": "2.958.0",
    "babel-jest": "27.0.6",
    "babel-loader": "8.2.2",
    "dotenv": "10.0.0",
    "eslint": "7.32.0",
    "jest": "26.6.3",
    "lint-staged": "11.1.1",
    "netlify-cli": "5.4.3",
    "node-forge": "0.10.0",
    "nodemailer": "6.6.3",
    "nodemon": "2.0.12",
    "prettier": "2.3.2",
    "ts-jest": "26.5.6",
    "typescript": "4.3.5",
    "webpack": "5.47.1",
    "webpack-cli": "4.7.2",
    "winston": "3.3.3"
  },
  "lint-staged": {
    ".{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "scripts": {
    "build": "webpack",
    "dev": "UNLOCK_ENV=dev nodemon src/server.js --exec babel-node",
    "test": "UNLOCK_ENV=test jest",
    "lint": "eslint --ext .ts,.js src/",
    "ci": "yarn test && yarn lint",
    "deploy-netlify": "./scripts/deploy-netlify.sh"
  },
