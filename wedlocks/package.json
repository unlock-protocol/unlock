  "version": "1.0.0",
  "description": "The email server for Unlock",
  "main": "src/server.js",
  "dependencies": {
    "@babel/cli": "7.14.8",
    "@babel/core": "7.15.0",
    "@babel/node": "7.14.9",
    "@babel/preset-env": "^7.15.6",
    "@typescript-eslint/parser": "4.30.0",
    "@unlock-protocol/eslint-config": "workspace:./packages/eslint-config",
    "aws-sdk": "2.981.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "27.1.0",
    "babel-loader": "8.2.2",
    "dotenv": "10.0.0",
    "eslint": "7.32.0",
    "jest": "27.2.1",
    "lint-staged": "11.1.2",
    "node-forge": "0.10.0",
    "nodemailer": "6.6.3",
    "nodemon": "2.0.12",
