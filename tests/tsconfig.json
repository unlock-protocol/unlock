{
  "extends": "@unlock-protocol/tsconfig",
  "compilerOptions": {
    "outDir": "./dist",
    "rootDirs": ["./src", "./test"],
    "resolveJsonModule": true,
    "noImplicitReturns": false,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "exclude": ["dist", "node_modules"],
  "include": [
    "./test",
    "./src"
  ]
}
