{
  "name": "smart-contract-extensions",
  "version": "0.0.1",
  "description": "Contracts which interact with other contracts",
  "scripts": {
    "build": "yarn compile",
    "compile": "truffle compile",
    "test": "truffle test",
    "lint": "solium -d ./contracts/ && eslint .",
    "lintFix": "solium -d ./contracts/ --fix && eslint . --fix",
    "coverage": "truffle run coverage",
    "ganache": "ganache-cli",
    "ci": "yarn build && yarn test && yarn coverage"
  },
  "files": [
    "build",
    "contracts",
    "src",
    "index.js"
  ],
  "engines": {
    "node": "^10"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unlock-protocol/unlock.git"
  },
  "keywords": [
    "Solidity",
    "Uniswap",
    "Ethereum",
    "ETH"
  ],
  "author": "HardlyDifficult",
  "license": "MIT",
  "dependencies": {
    "@openzeppelin/contracts": "3.1.0",
    "@openzeppelin/test-helpers": "0.5.6",
    "@openzeppelin/upgrades": "2.8.0",
    "@typescript-eslint/eslint-plugin": "3.10.1",
    "@typescript-eslint/parser": "4.3.0",
    "@unlock-protocol/unlock-abi-7": "1.0.3",
    "@vue/cli": "4.5.4",
    "babel-eslint": "10.1.0",
    "bignumber.js": "9.0.0",
    "chai": "4.2.0",
    "coveralls": "3.1.0",
    "erc1820": "0.0.2",
    "eslint": "7.8.1",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-json": "2.1.2",
    "eslint-plugin-markdown": "1.0.2",
    "eslint-plugin-prettier": "3.1.4",
    "eth-gas-reporter": "0.2.17",
    "ganache-cli": "6.10.2",
    "hardlydifficult-eth": "1.1.2",
    "prettier": "2.1.1",
    "solidity-coverage": "0.7.10",
    "solium": "1.2.5",
    "truffle": "5.1.43",
    "truffle-assertions": "0.9.2",
    "truffle-flattener": "1.4.4"
  }
}
