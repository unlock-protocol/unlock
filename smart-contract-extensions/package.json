{
  "name": "smart-contract-extensions",
  "version": "0.0.1",
  "description": "Contracts which interact with other contracts",
  "scripts": {
    "build": "yarn compile",
    "compile": "truffle compile",
    "test": "truffle test",
    "lint": "solium -d ./contracts/ && eslint .",
    "lintFix": "solium -d ./contracts/ --fix && eslint . --fix",
    "coverage": "truffle run coverage && cat coverage/lcov.info | coveralls",
    "ganache": "ganache-cli",
    "ci": "yarn build && yarn test && yarn lint"
  },
  "files": [
    "build",
    "contracts",
    "src",
    "index.js"
  ],
  "engines": {
    "node": "^10"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unlock-protocol/unlock.git"
  },
  "keywords": [
    "Solidity",
    "Uniswap",
    "Ethereum",
    "ETH"
  ],
  "author": "HardlyDifficult",
  "license": "MIT",
  "dependencies": {
    "@openzeppelin/contracts": "2.5.0",
    "@openzeppelin/test-helpers": "0.5.4",
    "@openzeppelin/upgrades": "2.7.1",
    "@typescript-eslint/eslint-plugin": "2.22.0",
    "@typescript-eslint/parser": "2.22.0",
    "@vue/cli": "4.2.3",
    "babel-eslint": "10.1.0",
    "bignumber.js": "9.0.0",
    "chai": "4.2.0",
    "coveralls": "3.0.9",
    "erc1820": "0.0.2",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-plugin-json": "2.1.0",
    "eslint-plugin-markdown": "1.0.2",
    "eslint-plugin-prettier": "3.1.2",
    "eth-gas-reporter": "0.2.15",
    "ganache-cli": "6.9.1",
    "hardlydifficult-ethereum-contracts": "0.10.0",
    "prettier": "1.19.1",
    "solidity-coverage": "0.7.2",
    "solium": "1.2.5",
    "truffle": "5.1.16",
    "truffle-assertions": "0.9.2",
    "unlock-abi-7": "1.0.2"
  }
}
