{
  "name": "locksmith",
  "version": "1.0.0",
  "description": "an unlock storage service",
  "main": "server.js",
  "scripts": {
    "prestart": "yarn build",
    "start": "node -r ./src/utils/envLoader.js ./build/server.js",
    "predev": "NODE_ENV=development yarn db:migrate",
    "dev": "tsc-watch --onSuccess 'yarn start' --onFailure 'echo Beep! Compilation Failed'",
    "pretest": "NODE_ENV=test yarn run db:migrate",
    "test": "jest --forceExit --runInBand",
    "lint": "eslint --ext .ts,.js src/ __tests__/",
    "db:migrate": "sequelize --config ./config/sequelize.config.js db:migrate",
    "preci": "yarn lint",
    "ci": "yarn test",
    "build": "tsc",
    "tsc": "tsc",
    "predist": "yarn build",
    "dist": "sh ./scripts/dist.sh",
    "prefulfillment:prepare": "yarn build",
    "fulfillment:prepare": "node ./scripts/fulfillment_prepare",
    "authorize": "./scripts/authorize-lock"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix"
    ]
  },
  "engines": {
    "node": "^10"
  },
  "author": "Unlock Inc",
  "license": "ISC",
  "dependencies": {
    "@opencensus/core": "0.0.22",
    "@opencensus/exporter-jaeger": "0.0.22",
    "@opencensus/nodejs": "0.0.22",
    "@types/cors": "2.8.6",
    "@types/eth-sig-util": "2.1.0",
    "@types/ethereumjs-util": "5.2.0",
    "@types/express": "4.17.7",
    "@types/express-serve-static-core": "4.17.8",
    "@types/isomorphic-fetch": "0.0.35",
    "@types/jest": "25.2.3",
    "@types/nock": "10.0.3",
    "@types/node": "12.12.50",
    "@types/request-promise-native": "1.0.17",
    "@types/sequelize": "4.28.9",
    "@types/stripe": "7.13.24",
    "@types/supertest": "2.0.10",
    "@unlock-protocol/unlock-js": "0.12.2",
    "apollo-cache-inmemory": "1.6.6",
    "apollo-client": "2.6.10",
    "apollo-datasource-graphql": "1.3.2",
    "apollo-link-http": "1.5.17",
    "apollo-server": "2.15.1",
    "apollo-server-express": "2.15.1",
    "aws-sdk": "2.714.0",
    "babel-jest": "26.1.0",
    "body-parser": "1.19.0",
    "coinbase": "2.0.8",
    "color-scheme": "1.0.1",
    "cors": "2.8.5",
    "cross-env": "7.0.2",
    "cross-fetch": "3.0.5",
    "dotenv": "8.2.0",
    "eslint": "7.4.0",
    "eth-sig-util": "2.5.3",
    "ethereumjs-util": "6.2.0",
    "ethers": "4.0.47",
    "express": "4.17.1",
    "express-jwt": "6.0.0",
    "graphql": "15.3.0",
    "graphql-tag": "2.10.4",
    "isomorphic-fetch": "2.2.1",
    "jest": "25.5.4",
    "lodash.isequal": "4.5.0",
    "multer": "1.4.2",
    "multer-s3": "2.9.0",
    "nock": "12.0.3",
    "node-mocks-http": "1.8.1",
    "pg": "8.3.0",
    "random-words": "1.1.1",
    "reflect-metadata": "0.1.13",
    "request": "2.88.2",
    "request-promise-native": "1.0.8",
    "run-script-os": "1.1.1",
    "sequelize": "5.22.3",
    "sequelize-cli": "6.2.0",
    "sequelize-typescript": "1.1.0",
    "stripe": "7.15.0",
    "supertest": "4.0.2",
    "ts-jest": "26.1.2",
    "typescript": "3.9.6",
    "unlock-abi-1-1": "1.1.1",
    "winston": "3.3.3",
    "yargs": "15.4.1"
  },
  "devDependencies": {
    "nodemon": "2.0.4",
    "supertest": "4.0.2",
    "tsc-watch": "4.2.9"
  },
  "jest": {
    "testMatch": [
      "**/__tests__/**/*.test.js?(x)"
    ]
  }
}
