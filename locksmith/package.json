{
  "name": "locksmith",
  "version": "1.0.0",
  "description": "an unlock storage service",
  "main": "server.js",
  "scripts": {
    "prestart": "npm run build",
    "start": "node ./build/server.js",
    "predev": "NODE_ENV=development npm run db:migrate",
    "dev": "tsc-watch --onSuccess 'node build/server.js' --onFailure 'echo Beep! Compilation Failed'",
    "pretest": "NODE_ENV=test sequelize db:migrate",
    "test": "jest --forceExit",
    "lint": "eslint --ext .ts,.js src/ __tests__/",
    "db:migrate": "sequelize db:migrate",
    "preci": "npm run lint",
    "ci": "npm test",
    "build": "tsc",
    "tsc": "tsc",
    "predist": "npm run build",
    "dist": "sh ./scripts/dist.sh",
    "prefulfillment:prepare": "npm run build",
    "fulfillment:prepare": "node ./scripts/fulfillment_prepare",
    "authorize": "./scripts/authorize-lock"
  },
  "lint-staged": {
    "linters": {
      "*.{js,ts}": [
        "eslint --fix",
        "git add"
      ]
    }
  },
  "author": "Unlock Inc",
  "license": "ISC",
  "dependencies": {
    "@opencensus/core": "0.0.17",
    "@opencensus/exporter-jaeger": "0.0.17",
    "@opencensus/nodejs": "0.0.17",
    "@types/cors": "2.8.6",
    "@types/eth-sig-util": "2.1.0",
    "@types/ethereumjs-util": "5.2.0",
    "@types/express": "4.17.1",
    "@types/express-serve-static-core": "4.16.9",
    "@types/jest": "24.0.18",
    "@types/nock": "10.0.3",
    "@types/node": "12.7.5",
    "@types/request-promise-native": "1.0.16",
    "@types/sequelize": "4.28.4",
    "@types/stripe": "6.32.2",
    "@types/supertest": "2.0.8",
    "@typescript-eslint/eslint-plugin": "2.3.0",
    "@unlock-protocol/unlock-js": "0.3.12",
    "aws-sdk": "2.533.0",
    "body-parser": "1.19.0",
    "cors": "2.8.5",
    "cross-env": "6.0.0",
    "dotenv": "8.1.0",
    "eth-sig-util": "2.4.4",
    "ethereumjs-util": "6.1.0",
    "ethers": "4.0.37",
    "express": "4.17.1",
    "express-jwt": "5.3.1",
    "jest": "24.9.0",
    "lodash.isequal": "4.5.0",
    "multer": "1.4.2",
    "multer-s3": "2.9.0",
    "nock": "11.3.5",
    "node-mocks-http": "1.8.0",
    "pg": "7.12.1",
    "random-words": "1.1.0",
    "reflect-metadata": "0.1.13",
    "request": "2.88.0",
    "request-promise-native": "1.0.7",
    "run-script-os": "1.0.7",
    "sequelize": "5.19.0",
    "sequelize-cli": "5.5.1",
    "sequelize-typescript": "1.0.0",
    "stripe": "7.9.1",
    "supertest": "4.0.2",
    "truffle-hdwallet-provider": "1.0.17",
    "ts-jest": "24.1.0",
    "typescript": "3.6.3",
    "unlock-abi-1-1": "1.0.0",
    "winston": "3.2.1",
    "yargs": "14.0.0"
  },
  "devDependencies": {
    "nodemon": "1.19.2",
    "sqlite3": "4.1.0",
    "supertest": "4.0.2",
    "tsc-watch": "4.0.0"
  },
  "jest": {
    "testMatch": [
      "**/__tests__/**/*.test.js?(x)"
    ]
  }
}
